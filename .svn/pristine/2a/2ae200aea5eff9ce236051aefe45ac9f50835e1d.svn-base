package com.wangqingbo.entity;

/**
 * 项目的费用信息表
 * @author Administrator
 *
 */
public class ProjectFeeInfo {

	// 主键
	private String feeId;
	// 项目id
	private String projectId;
	// 安装费用
	private Double azfy;
	// 建设费用
	private Double jsfy;
	// 其他费用
	private Double qtfy;
	public String getFeeId() {
		return feeId;
	}
	public void setFeeId(String feeId) {
		this.feeId = feeId;
	}
	public String getProjectId() {
		return projectId;
	}
	public void setProjectId(String projectId) {
		this.projectId = projectId;
	}
	public Double getAzfy() {
		return azfy;
	}
	public void setAzfy(Double azfy) {
		this.azfy = azfy;
	}
	public Double getJsfy() {
		return jsfy;
	}
	public void setJsfy(Double jsfy) {
		this.jsfy = jsfy;
	}
	public Double getQtfy() {
		return qtfy;
	}
	public void setQtfy(Double qtfy) {
		this.qtfy = qtfy;
	}
	
	
}
